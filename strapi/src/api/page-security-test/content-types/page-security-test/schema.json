{
  "kind": "collectionType",
  "collectionName": "page_security_tests",
  "info": {
    "singularName": "page-security-test",
    "pluralName": "page-security-tests",
    "displayName": "PageSecurityTest",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "accepted_regulations": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "website": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "tests_results": {
      "type": "component",
      "repeatable": true,
      "component": "custom.test-result"
    },
    "error_msg": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "waiting",
        "running",
        "finished",
        "failed"
      ],
      "default": "waiting",
      "required": true
    },
    "accepted_marketing": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "personal_data_processing": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
